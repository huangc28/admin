{
  "presets": [
    ["es2015", { "modules": false }],
    "stage-1",
    "react"
  ],
  "env": {
  	"development": {
      "presets": ["es2015", "stage-1", "react"]
    },
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    },
    "test": {
      "presets": ["es2015", "stage-1", "react"]
    }
  },
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}
